<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/styles.css" />

    <title>All Messages</title>
  </head>
  <body>
    <header>
      <h1>Messages</h1>
    </header>
    <ul>
      <% messages.forEach(message => { %> <%- include('message', { message:
      message }) %> <% }) %>
    </ul>
    <a href="/messages/new"><button>New message</button></a>
  </body>
</html>
